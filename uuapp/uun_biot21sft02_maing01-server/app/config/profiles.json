{
  "{asid}": {
    "profileList": ["AsidAuthorities","AsidExecutives","AsidLicenseOwner","Public"],
    "useCaseMap": {}
  },
  "*": {
    "profileList": ["Authorities","Gateways","Auditors","AwidLicenseOwner","Public"],
    "useCaseMap": {
      "sys/uuAppWorkspace/init": {
        "sysStateList": ["created"],
        "profileList": ["AwidLicenseOwner"]
      },
      "defaultUve": {
        "profileList": ["Public"]
      },
      "sys/uuAppWorkspace/initUve": {
        "sysStateList": ["created"],
        "profileList": ["Public","AwidLicenseOwner"]
      },
      "gateway/create": {
        "sysStateList": ["active"],
        "profileList": ["Authorities"]
      },
      "dataset/get": {
        "sysStateList": ["active"],
        "profileList": ["Authorities","Gateways"]
      },
      "gateway/update": {
        "sysStateList": ["active"],
        "profileList": ["Authorities"]
      }
    }
  }
}